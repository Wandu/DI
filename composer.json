{
    "name": "wandu/di",
    "description": "Dependency Injection Container. Support Auto-Resolve and Auto-Wiring. Also Perfect Fit to PSR-11.",
    "keywords": ["di", "ioc", "auto resolve", "auto wiring", "dependency injection", "wandu"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "psr/container": "^1",
        "psr/simple-cache": "^1",
        "wandu/config": "^4.0",
        "wandu/reflection": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\DI\\": ""
        },
        "files" : [
            "functions.php"
        ]
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    },
    "suggest": {
        "doctrine/annotations": "for the AutoWired annotation."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
