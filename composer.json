{
    "name": "wandu/di",
    "description": "Dependency Injection Container. Support Auto-Resolve and Auto-Wiring.",
    "keywords": ["di", "ioc", "auto resolve", "auto wiring", "dependency injection", "wandu"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "wandu/reflection": "^0.2",
        "container-interop/container-interop": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\DI\\": ""
        },
        "files" : [
            "functions.php"
        ]
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    },
    "suggest": {
        "doctrine/annotations": "for the AutoWired annotation."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
